                    <Editor.Scripts.GUI.Factories.TextElements.Text tabindex="-1" text="Select your currently used RenderPipelineAsset. A newly created RendererData will be added to it in order to render transparent images." style="white-space: normal; margin-left: 4px;" />
                    <Editor.Scripts.GUI.Factories.TextElements.Text tabindex="-1" text="Universal Render Pipeline Asset" style="margin-top: 20px; margin-left: 4px; white-space: normal; -unity-font-style: bold;" />
                    <uie:ObjectField type="UnityEngine.Rendering.Universal.UniversalRenderPipelineAsset, Unity.RenderPipelines.Universal.Runtime" name="PipelineAsset" tooltip="RenderPipelineAsset used by your project." />
                </ui:GroupBox>
                <ui:GroupBox name="Step2" style="margin-top: 5px; margin-right: 5px; margin-bottom: 5px; margin-left: 5px; padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0;">
                    <Editor.Scripts.GUI.Factories.Headings.HeadingPrimary tabindex="-1" text="Step 2" style="-unity-text-align: middle-center; white-space: normal; font-size: 14px; -unity-font-style: bold;" />
                    <Editor.Scripts.GUI.Factories.TextElements.Text tabindex="-1" text="A duplication of your template will be created and setup to be used as the renderer when rendering camera captures." style="white-space: normal; margin-left: 4px;" />
                    <Editor.Scripts.GUI.Factories.TextElements.Text tabindex="-1" text="Universal Renderer Data" style="margin-top: 20px; margin-left: 4px; white-space: normal; -unity-font-style: bold;" />
                    <uie:ObjectField label="Template" type="UnityEngine.Rendering.Universal.UniversalRendererData, Unity.RenderPipelines.Universal.Runtime" name="RendererData" tooltip="RendererData used for duplication." />
                </ui:GroupBox>
                <ui:GroupBox name="Step3" style="margin-top: 5px; margin-right: 5px; margin-bottom: 5px; margin-left: 5px; padding-top: 0; padding-right: 0; padding-bottom: 0; padding-left: 0;">
                    <Editor.Scripts.GUI.Factories.Headings.HeadingPrimary tabindex="-1" text="Step 3" style="-unity-text-align: middle-center; white-space: normal; font-size: 14px; -unity-font-style: bold;" />
                    <Editor.Scripts.GUI.Factories.TextElements.Text tabindex="-1" text="Choose a folder to save the duplicated data and the modified shader." style="white-space: normal; margin-left: 4px;" />
